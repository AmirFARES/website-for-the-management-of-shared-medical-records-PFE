 <!-- editAccModal -->
 <div class="modal fade" id="editAccModal" tabindex="-1" aria-labelledby="editAccModalLabel"
 aria-hidden="true">
 <div class="modal-dialog">
     <div class="modal-content">
         <div class="modal-header">
             <h5 class="modal-title" id="editAccModalLabel">Modifier les informations du dossier</h5>
             <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
         </div>
         <form action="/emergency/patient/<%=idMalade%>/edit" method="POST">
             <div class="modal-body">
                 <div class="container">
                  <div class="row mb-2">
                    <select name="wilayaDomicile" class="form-control">
                      <% for(let i=0;i<wilayas.length;i++){ %>
                          <% if(i==idWilayaDom-1){ %>
                              <option value="<%=wilayas[i].wilaya_code %>" selected>
                                  <%=wilayas[i].wilayaFrName %>
                              </option>
                              <% }else{ %>
                                  <option value="<%=wilayas[i].wilaya_code %>">
                                      <%=wilayas[i].wilayaFrName %>
                                  </option>
                                  <% } %>
                                      <% } %>
                    </select>
                  </div>
                  <div class="row mb-2">
                    <input type="text" class="form-control" id="dairaDomicile" name="dairaDomicile" placeholder="Daira">
                  </div>
                  <div class="row mb-2">
                    <input type="text" class="form-control" id="communeDomicile" name="communeDomicile" placeholder="Commune">
                   </div>
                   <div class="row mb-2">
                    <input type="text" class="form-control" id="quartierDomicile" name="quartierDomicile" placeholder="Quartier">
                   </div>
                 </div>
                 <div class="modal-footer">
                   <div class="row text-muted">si vous laissez un champ cela ne changera pas</div>
                   <div class="row"><button type="button" class="btn btn-secondary col m-1" data-bs-dismiss="modal">Fermer</button>
                       <button type="submit" class="btn btn-primary col m-1">Modifier</button></div> 
                 </div>
             </div>
         </form>
     </div>
 </div>
</div>
</body>

